{
  "multipart": [
    {
      "apply": {
        "model": "buildcraftfactory:block/chute"
      },
      "when": {
        "facing": "up"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute",
        "x": 180
      },
      "when": {
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute",
        "x": 90,
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute",
        "x": 90,
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute",
        "x": 90,
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute",
        "x": 90
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute_connected",
        "x": 180
      },
      "when": {
        "connected_up": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute_connected"
      },
      "when": {
        "connected_down": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute_connected",
        "x": 90,
        "y": 270
      },
      "when": {
        "connected_east": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute_connected",
        "x": 90
      },
      "when": {
        "connected_south": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute_connected",
        "x": 90,
        "y": 90
      },
      "when": {
        "connected_west": "true"
      }
    },
    {
      "apply": {
        "model": "buildcraftfactory:block/chute_connected",
        "x": 90,
        "y": 180
      },
      "when": {
        "connected_north": "true"
      }
    }
  ]
}