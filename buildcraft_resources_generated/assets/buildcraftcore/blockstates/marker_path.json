{
  "multipart": [
    {
      "apply": {
        "model": "buildcraftcore:block/marker_path"
      },
      "when": {
        "facing": "up"
      }
    },
    {
      "apply": {
        "model": "buildcraftcore:block/marker_path",
        "x": 180
      },
      "when": {
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "buildcraftcore:block/marker_path",
        "x": 90,
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "buildcraftcore:block/marker_path",
        "x": 90,
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "buildcraftcore:block/marker_path",
        "x": 90,
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "buildcraftcore:block/marker_path",
        "x": 90
      },
      "when": {
        "facing": "north"
      }
    }
  ]
}